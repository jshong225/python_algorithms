    "def fib(n):\n",
    "    # base case\n",
    "    if n < 3:\n",
    "        return 1\n",
    "    \n",
    "    # recursive case\n",
    "    return fib(n - 1) + fib(n - 2)\n",
    "    \n",
    "# 테스트: fib(1)부터 fib(10)까지 출력\n",
    "for i in range(1, 11):\n",
    "    print(fib(i))"
   ]
  }
